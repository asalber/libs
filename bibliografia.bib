	journal = {Spectrochimica Acta Part B: Atomic Spectroscopy},
	author = {Bellou, Elli and Gyftokostas, Nikolaos and Stefas, Dimitrios and Gazeli, Odhisea and Couris, Stelios},
	month = jan,
	year = {2020},
	keywords = {Classification, Laser-induced breakdown spectroscopy, LIBS, Machine Learning, Olive oil},
	pages = {105746},
	file = {Bellou et al. - 2020 - Laser-induced breakdown spectroscopy assisted by m.pdf:/home/alf/Zotero/storage/UT57L26F/Bellou et al. - 2020 - Laser-induced breakdown spectroscopy assisted by m.pdf:application/pdf},
}

