	month = sep,
	year = {2019},
	note = {Number: 3
Publisher: Multidisciplinary Digital Publishing Institute},
	keywords = {LIBS, identification of polymers, laser produced plasmas, laser-induced breakdown, machine learning, polymers},
	pages = {79},
	file = {Full Text PDF:/home/alf/Zotero/storage/AV9JGYSR/Stefas et al. - 2019 - Laser-Induced Breakdown Spectroscopy Assisted by M.pdf:application/pdf},
