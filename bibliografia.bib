	author = {Gaudiuso, Rosalba and Melikechi, Noureddine and Abdel-Salam, Zienab A. and Harith, Mohamed A. and Palleschi, Vincenzo and Motto-Ros, Vincent and Busser, Benoit},
	month = feb,
	year = {2019},
	pages = {123--148},
	file = {Gaudiuso et al. - 2019 - Laser-induced breakdown spectroscopy for human and.pdf:/home/alf/Zotero/storage/R68KAWBK/Gaudiuso et al. - 2019 - Laser-induced breakdown spectroscopy for human and.pdf:application/pdf;ScienceDirect Snapshot:/home/alf/Zotero/storage/KNR8I2VY/S0584854718303513.html:text/html},
}

@article{motto-ros_critical_2018,
	title = {Critical aspects of data analysis for quantification in laser-induced breakdown spectroscopy},
	volume = {140},
	issn = {0584-8547},
	url = {https://www.sciencedirect.com/science/article/pii/S0584854717304044},
	doi = {10.1016/j.sab.2017.12.004},
	abstract = {In this study, a collaborative contest focused on LIBS data processing has been conducted in an original way since the participants did not share the same samples to be analyzed on their own LIBS experiments but a set of LIBS spectra obtained from one single experiment. Each participant was asked to provide the predicted concentrations of several elements for two glass samples. The analytical contest revealed a wide diversity of results among participants, even when the same spectral lines were considered for the analysis. Then, a parametric study was conducted to investigate the influence of each step during the data processing. This study was based on several analytical figures of merit such as the determination coefficient, uncertainty, limit of quantification and prediction ability (i.e., trueness). Then, it was possible to interpret the results provided by the participants, emphasizing the fact that the type of data extraction, baseline modeling as well as the calibration model play key roles in the quantification performance of the technique. This work provides a set of recommendations based on a systematic evaluation of the quantification procedure with the aim of optimizing the methodological steps toward the standardization of LIBS.},
	urldate = {2024-04-24},
	journal = {Spectrochimica Acta Part B: Atomic Spectroscopy},
	author = {Motto-Ros, V. and Syvilay, D. and Bassel, L. and Negre, E. and Trichard, F. and Pelascini, F. and El Haddad, J. and Harhira, A. and Moncayo, S. and Picard, J. and Devismes, D. and Bousquet, B.},
	month = feb,
	year = {2018},
	keywords = {LIBS, Assessment, Collaborative contest, Data extraction, Parametric study, Quantitative},
	pages = {54--64},
	file = {Motto-Ros et al. - 2018 - Critical aspects of data analysis for quantificati.pdf:/home/alf/Zotero/storage/4U4A3WE8/Motto-Ros et al. - 2018 - Critical aspects of data analysis for quantificati.pdf:application/pdf;ScienceDirect Snapshot:/home/alf/Zotero/storage/53MAPIYA/S0584854717304044.html:text/html},
}

